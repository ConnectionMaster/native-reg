{
  "name": "native-reg",
  "description": "Native in-process low-level windows registry access.",
  "version": "0.3.4",
  "repository": "https://github.com/simonbuchan/native-reg",
  "bugs": "https://github.com/simonbuchan/native-reg/issues",
  "license": "MIT",
  "os": [
    "win32"
  ],
  "keywords": [
    "windows",
    "win32",
    "registry",
    "native"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "gypfile": false,
  "files": [
    "lib",
    "src",
    "binding.gyp",
    "*.cc",
    "test.js"
  ],
  "scripts": {
    "install": "npm run build:c",
    "prepare": "npm run rebuild",
    "rebuild": "npm run clean && npm run build && npm run test",
    "clean": "rimraf build lib prebuilds",
    "build": "npm run build:js && npm run build:c",
    "build:js": "tsc -p src",
    "build:c": "node-gyp rebuild",
    "test": "node test"
  },
  "dependencies": {
    "node-addon-api": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^10.11.0",
    "rimraf": "^3.0.0",
    "typescript": "~3.7.3"
  }
}
